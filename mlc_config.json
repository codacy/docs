{
    "ignorePatterns": [
        {
            "pattern": "^(?!http).+\\.md(#.+)?$"
        },
        {
            "pattern": "^images\\"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/",
            "replacement": "https://docs.codacy.com/"
        }
    ],
    "retryOn429": true,
    "retryCount": 2,
    "fallbackRetryDelay": "60s"
}